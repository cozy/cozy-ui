    "cssnano": "4.1.11",
    "cssnano-preset-advanced": "4.0.8",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.11.0",
    "eslint-config-cozy-app": "4.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.1",
    "focus-trap-react": "^6.0.0",
    "git-directory-deploy": "1.5.1",
    "handlebars": "^4.7.6",
