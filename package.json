    "livereload": "^0.9.1",
    "lodash": "4.17.21",
    "michelangelo": "https://github.com/cozy/michelangelo.git",
    "mini-css-extract-plugin": "0.6.0",
    "nodemon": "1.19.4",
    "npm-run-all": "4.1.5",
    "postcss-cli": "6.1.3",
    "postcss-loader": "2.1.6",
    "pretty": "2.0.0",
